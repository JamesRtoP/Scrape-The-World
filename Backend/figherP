<script type="text/javascript">
    $(function () {
        const second = 1000,
            minute = second * 60,
            hour = minute * 60,
            day = hour * 24;

        var countDown = moment.tz("2025-01-25 12:00:00", 'America/New_York').valueOf();

        var x = setInterval(function() {
            var now = moment().tz('America/New_York').valueOf();
            var distance = countDown - now;

            //do something later when date is reached
            if (distance > 0) {
                document.getElementById("days").innerText = Math.floor(distance / (day)),
                document.getElementById("hours").innerText = Math.floor((distance % (day)) / (hour)),
                document.getElementById("minutes").innerText = Math.floor((distance % (hour)) / (minute)),
                document.getElementById("seconds").innerText = Math.floor((distance % (minute)) / second);
            }else{
                //var html = '<span class="live_now"><a href="/challenger-watch">LIVE NOW</a></span>';
                var html = '<span class="live_now"><a href="/watch">LIVE NOW</a></span>';

                $("#countdown_time").html(html);
                clearInterval(x);
            }
        }, 0);
    });
</script>

