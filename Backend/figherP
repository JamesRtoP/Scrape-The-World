    <script>
        $(document).ready(function() {
            $('.changeStats').on('click', function() {
                $('.changeStats').removeClass('font-weight-bold');
                $(this).addClass('font-weight-bold');
            });
        });
        // Run wins counter, then losses
        // PASS THE WINS AND LOSSES IN HERE
        var totalWins = 34;
        var totalLosses = 6;

        var numAnim = new CountUp("totalWins", 0, totalWins);
        var numAnim2 = new CountUp("totalLosses", 0, totalLosses);
        if (!numAnim.error) {
            setTimeout(function() {
                numAnim.start(function() {
                    // Run losses when wins finish
                    numAnim2.start();
                });
            }, 2000);
        } else {
            console.error(numAnim.error);
        }
    </script>


